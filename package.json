{
  "name": "logbubble",
  "version": "0.2.0",
  "description": "LogBubble: Web floating logger for network and console logs",
  "main": "dist/init.js",
  "types": "dist/init.d.ts",
  "exports": {
    ".": {
      "types": "./dist/init.d.ts",
      "import": "./dist/init.js",
      "require": "./dist/init.js"
    },
    "./vite-logbubble-plugin": {
      "types": "./dist/vite-logbubble-plugin.d.ts",
      "import": "./dist/vite-logbubble-plugin.js",
      "require": "./dist/vite-logbubble-plugin.js"
    },
    "./auto": {
      "types": "./dist/auto.d.ts",
      "import": "./dist/auto.js",
      "require": "./dist/auto.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "dist/auto.js",
    "dist/init.js"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "typescript": "^5.0.0",
    "vite": "^7.3.1"
  },
  "keywords": [
    "logbubble",
    "network",
    "console",
    "logger",
    "web",
    "devtools",
    "ui",
    "bubble"
  ],
  "author": "Deepak Kumar",
  "license": "MIT"
}
